<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bananatigeer.MainController">
   <children>
      <Button layoutX="123.0" layoutY="265.0" mnemonicParsing="false" onAction="#loadFile" prefHeight="56.0" prefWidth="152.0" text="Select File" />
      <Button fx:id="btn_parseFile" disable="true" layoutX="325.0" layoutY="265.0" mnemonicParsing="false" onAction="#processFile" prefHeight="56.0" prefWidth="152.0" text="Parse File" />
      <BorderPane layoutX="92.0" layoutY="53.0" prefHeight="200.0" prefWidth="417.0">
         <center>
            <Label fx:id="lbl_path" alignment="CENTER" prefHeight="44.0" prefWidth="417.0" text="Select a file.." BorderPane.alignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font></Label>
         </center>
         <bottom>
            <Label fx:id="lbl_savePath" alignment="CENTER" prefHeight="66.0" prefWidth="417.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="11.0" />
               </font>
            </Label>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
